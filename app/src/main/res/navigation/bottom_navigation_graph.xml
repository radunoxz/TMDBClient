<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/bottom_navigation_graph"
    app:startDestination="@id/homeFragment2">
    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.example.tmdbclient.presentation.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment2_to_movieFragment"
            app:destination="@id/movieFragment" />
        <action
            android:id="@+id/action_homeFragment2_to_tvShowFragment"
            app:destination="@id/tvShowFragment" />
        <action
            android:id="@+id/action_homeFragment2_to_artistFragment"
            app:destination="@id/artistFragment" />
    </fragment>
    <fragment
        android:id="@+id/movieFragment"
        android:name="com.example.tmdbclient.presentation.movie.MovieFragment"
        android:label="MovieFragment" />
    <fragment
        android:id="@+id/tvShowFragment"
        android:name="com.example.tmdbclient.presentation.tvshow.TvShowFragment"
        android:label="TvShowFragment" />
    <fragment
        android:id="@+id/artistFragment"
        android:name="com.example.tmdbclient.presentation.artist.ArtistFragment"
        android:label="ArtistFragment" />
</navigation>
